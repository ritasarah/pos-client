        <activity
            android:name=".Belanja"
            android:label="@string/title_activity_belanja" >
        </activity>
        <activity
            android:name=".Riwayat"
            android:label="@string/title_activity_riwayat" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
